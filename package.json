{
  "name": "studio37-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "repo:dev": "turbo run dev --parallel",
    "repo:build": "turbo run build",
    "repo:lint": "turbo run lint",
    "repo:typecheck": "turbo run typecheck",
    "web:dev": "turbo run dev --filter=web",
    "workflow:dev": "turbo run dev --filter=workflow",
    "import:training": "tsx scripts/import-website-content.ts"
  },
  "dependencies": {
    "resend": "^4.0.1",
    "twilio": "^5.3.5",
    "@react-email/components": "^0.0.25",
    "@react-email/render": "^1.0.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  },
  "devDependencies": {
    "turbo": "^1.11.2"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "not dead"
  ]
}
